{
    "compilerOptions": {
        "jsx": "react",
        "sourceMap": true,
        "target": "es2015",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "paths": {
            "src/*": ["./src/*"],
            "components/*": ["./src/components/*.android", "./src/components/*.ios", "./src/components/*"],
            "resources/*": ["./src/resources/*"]
        }
    }
}